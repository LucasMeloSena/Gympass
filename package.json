{
  "name": "gympass",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start:prod": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/services",
    "test:watch": "vitest --dir src/services",
    "test:create-prisma-environment": "sudo npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:fast": "vitest run --dir src/http",
    "test:coverage": "vitest run --coverage",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "services:up": "docker compose -f src/infra/docker-compose.yaml up -d",
    "services:stop": "docker compose -f src/infra/docker-compose.yaml stop",
    "services:down": "docker compose -f src/infra/docker-compose.yaml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.14.12",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.4",
    "eslint": "8.57.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.17.0",
    "supertest": "7.0.0",
    "tsup": "8.2.3",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.12",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "zod": "3.23.8"
  }
}
